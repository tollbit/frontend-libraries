{
  "name": "@tollbit/client",
  "version": "0.0.2",
  "description": "Tollbit SDK for usage with node",
  "homepage": "https://tollbit.com/",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "build": "tsc -p tsconfig.base.json && tsc -p tsconfig.cjs.json",
    "type-check": "tsc --noEmit -p tsconfig.base.json",
    "format:check": "prettier --check .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "tollbit",
    "sdk",
    "node"
  ],
  "author": {
    "name": "Tollbit.",
    "email": "team@tollbit.com",
    "url": "git+https://github.com/tollbit/frontend-libraries.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.30",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "axios": "^1.6.8"
  }
}
